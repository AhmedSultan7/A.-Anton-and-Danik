#include<iostream>
#include<string>
using namespace std;
int main() {
 
	string s;
	int tc;
	cin >> tc;
	cin >> s;
	int A = 0, D = 0;
	for (int i = 0; i < tc; i++)
	{
		
		if (s[i] == 'A')
			A++;
		else
			D++;
 
	}
	if (A > D)
		cout << "Anton" << endl;
	else if (A < D)
			cout << "Danik" << endl;
    else cout << "Friendship" << endl;
 
	return 0;
}
